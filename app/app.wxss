/* S flex */
.wx-flex {
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.just-start {
  justify-content: flex-start;
}
.just-center {
  justify-content: center;
}
.just-end {
  justify-content: flex-end;
}
.just-around {
  justify-content: space-around;
}
.align-str {
  align-items: stretch;
}
.align-c {
  align-items:center;
}
.align-start {
  align-self: flex-start;
}
.align-end {
  align-self: flex-end;
}
.wx-flex-c {
  display: flex;
  flex-direction: column;
}
/* E flex */

/* S 页面内容可以共用的样式 */
.o-arrow-r {
  width: 16rpx;
  height: 16rpx;
  border-width: 6rpx 6rpx 0 0;
  border-color: #c7c7cc;
  border-style: solid;
  transform: matrix(0.71, 0.71, -0.71, 0.71, 0, 0);
  margin-right: 4rpx;
}
.btn-area {
  padding: 0 30rpx 30rpx;
}
.o-con {
  background: #fff;
  /*border: 2rpx solid #e5e5e5;*/
  border-left: none;
  border-right: none;
}
.bod-b{
  border-bottom: 2rpx solid #e5e5e5 !important;
}
.bod-t{
  border-top: 2rpx solid #e5e5e5 !important;
}
.bod-tb0{
  border-top:none;
  border-bottom:none;
}
.pt0{
  padding-top:0 !important;
}
.o-tip,.o-tip0{
  width: 100%;
  background: #e64340;
  color: #fff;
  font-size: 30rpx;
  text-align: center;
  padding: 20rpx;
  position: fixed;
  top: 100rpx;
}
.o-tip0{
  top:0;
}
.o-teaser{
  padding: 30rpx 30rpx 30rpx 0;
  margin-left: 30rpx;
}
.o-teaser:last-child{
  border-bottom:none;
}
.order-teaser{
  padding: 40rpx 40rpx 40rpx 0;
  margin-left: 30rpx;
}
.order-teaser:last-child{
  border-bottom:none;
}
.o-adr-e{
  width:480rpx;
  white-space:nowrap;
  overflow:hidden;
  text-overflow:ellipsis;
  padding-top:10rpx;
}
.radio-pad{
  padding-right:25rpx;
}
.send-icon{
  width:48rpx;
  height:48rpx;
  padding-right:30rpx;
}
.slide-image{
  width:100%;
}
.o-adr-s{
  border-left:2rpx solid #e5e5e5;
  padding:20rpx 0 20rpx 30rpx;
}
.o-cho-btn{
  padding-right:26rpx;
}
.pos-bot{
  position:fixed;
  bottom:0;
  width:100%;
  padding:16px 30rpx 30px;
  background:#efeff4;
}
