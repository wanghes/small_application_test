@import './base/base.wxss';
/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
}
.bod-box {
  box-sizing: border-box;
}
page {
  /*background: #efeff4;*/
}
.header {
  font-size: 28rpx;
  padding: 0 63rpx;
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  z-index: 10;
  background: #fff;
}
.nav-item {
  padding: 0 69rpx;
  color: #353535;
  height: 100rpx;
}
.nav-item-cur {
  border-bottom: 6rpx solid #1aad19;
  color: #1aad19;
}
.pos-re{
  position:relative;
}
/* S margin padding text */
.mb30 {
  margin-bottom: 30rpx;
}
.mt30{
  margin-top:30rpx;
}
.mt20{
  margin-top:20rpx;
}
.mt60 {
  margin-top: 60rpx;
}
.mt16 {
  margin-top: 16rpx;
}
.pt35{
  padding-top:35rpx;
}
.pt10{
  padding-top:10rpx;
}
.plr30{
  padding:0 30rpx 0;
}
.tl {
  text-align: left;
}
.tc {
  text-align: center;
}
.tr {
  text-align: right;
}
/* E margin padding text */

/* display */
.block {
  display: block;
}


/* S font */
.f48 {
  font-size: 48rpx;
}
.f36 {
  font-size: 36rpx;
}
.f34 {
  font-size: 34rpx;
}
.f32 {
